    "nprof": "^0.6.0",
    "repl-extra": "^0.1.3",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-json-schema": "^1.4.0",
    "codeclimate-test-reporter": "^0.4.0",
    "esdoc": "^0.4.8",
    "esdoc-node": "^1.0.0",
    "eslint": "^3.11.1",
    "istanbul": "^0.4.4",
    "mocha": "^3.2.0"
  }
