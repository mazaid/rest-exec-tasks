  "dependencies": {
    "joi": "^10.0.2",
    "lodash": "^4.16.4",
    "maf": "^0.5.0",
    "mazaid-error": "^0.3.1",
    "mazaid-exec": "^0.2.3",
    "mazaid-exec-task": "^0.2.0",
    "md5": "^2.2.1",
    "moment": "^2.15.1",
